VIEW "ISO.COUNTRIES_REGIONS" ( 
     "COUNTRY_CODE",
	 "COUNTRY_NAME",
	 "SUB_CODE",
	 "NAME",
	 "TYPE" ) AS SELECT
	 "SUBDIVISIONS_$0"."COUNTRY_CODE"."ALPHA2" AS "COUNTRY_CODE" ,
	 "SUBDIVISIONS_$0"."COUNTRY_CODE"."NAME" AS "COUNTRY_NAME" ,
	 "SUBDIVISIONS_$0"."SUB_CODE" ,
	 "SUBDIVISIONS_$0"."NAME" ,
	 "SUBDIVISIONS_$0"."TYPE" 
FROM "ISO.SUBDIVISIONS" AS "SUBDIVISIONS_$0" WITH READ ONLY