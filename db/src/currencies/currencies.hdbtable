COLUMN TABLE "ISO.CURRENCIES"(
	"ENTITY" NVARCHAR(80) COMMENT 'Entity Name',
	"CURRENCY" NVARCHAR(80) COMMENT 'Currency Name',
	"CODE" NVARCHAR(3) COMMENT 'Alpha Currency Code',
	"NCODE" NVARCHAR(3) COMMENT 'Numeric Currency Code',
	"MUNIT" NVARCHAR(3) COMMENT 'Minor Unit',
	"WITHDRAWL" NVARCHAR(20) COMMENT 'Withdrawl Date'
) 
COMMENT 'ISO-4217 Currencies'
UNLOAD PRIORITY 5 AUTO MERGE